#include "jorne.dtsi" // The shared dtsi file is included in the overlay

// Label of the kscan node in the dtsi
// col 7 is on the right most side of the board
// col 7 (right most) -> pin 21
// col 8 -> pin 20
// ...
// col 15 (left most side) -> pin 4
&default_transform { // Offset of 8 because the left side has 8 columns
    col-offset = <7>;
};
